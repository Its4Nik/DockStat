{
  "name": "@dockstat/typings",
  "version": "1.0.0",
  "exports": {
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "bun run ./build.ts",
    "dev": "bun run --watch ./build.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:jn": "biome lint --reporter=junit > lint-typings.xml",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dockerode": "^3.3.42"
  }
}
