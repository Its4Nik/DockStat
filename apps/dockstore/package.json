{
  "name": "dockstacks",
  "module": "index.ts",
  "extends": "../../tsconfig.base.json",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun build index.ts --outdir dist",
    "dev": "bun run index.ts",
    "make": "cd .generator && bun run convert.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:gh": "turbo run lint -- --reporter=github",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
