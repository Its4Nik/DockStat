    "@dockstat/sqlite-wrapper": "workspace:*",
    "@dockstat/typings": "workspace:*",
    "@react-router/fs-routes": "^7.9.1",
    "@react-router/node": "^7.9.1",
    "@react-router/serve": "^7.9.1",
    "@types/dockerode": "^3.3.43",
    "dockerode": "^4.0.8",
    "isbot": "^5.1.30",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "react-router-theme": "^1.2.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^20.19.14",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
