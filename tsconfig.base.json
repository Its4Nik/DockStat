{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false
  },
  "include": [],
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ],
  "references": [
    {
      "path": "./packages/docker-client"
    },
    {
      "path": "./packages/plugin-handler"
    },
    {
      "path": "./packages/sqlite-wrapper"
    },
    {
      "path": "./packages/typings"
    },
    {
      "path": "./packages/logger"
    },
    {
      "path": "./packages/utils"
    },
    {
      "path": "./packages/db"
    },
    {
      "path": "./packages/ui"
    }
  ]
}
