{
  "name": "@dockstat/dockstat",
  "private": true,
  "type": "module",
  "exports": {
    "./server.ts": "./app/.server"
  },
  "scripts": {
    "build": "bun --bun cross-env NODE_ENV=production react-router build --minify true ",
    "dev": "bun run --bun react-router dev",
    "start": "bun run --bun server.ts",
    "compile": "bun --bun build --compile --target=bun-linux-x64 --sourcemap ./server.ts --outfile build/server.bin && chmod +x build/server.bin",
    "typegen": "react-router typegen",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:gh": "turbo run lint -- --reporter=github",
    "check-types": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@dockstat/db": "workspace:*",
    "@dockstat/docker-client": "workspace:*",
    "@dockstat/logger": "latest",
    "@dockstat/plugin-handler": "workspace:*",
    "@dockstat/sqlite-wrapper": "workspace:*",
    "@dockstat/typings": "workspace:*",
    "@gsap/react": "^2.1.2",
    "@react-router/express": "^7.9.3",
    "@react-router/fs-routes": "^7.9.3",
    "@react-router/node": "^7.9.3",
    "@react-router/serve": "^7.9.3",
    "@types/dockerode": "^3.3.44",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "dockerode": "^4.0.9",
    "elysia-react-router": "^0.4.4",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "isbot": "^5.1.31",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.9.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.3",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "cross-env": "^10.1.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.8",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
