{
  "name": "Uptime-Kuma",
  "version": 1,
  "custom": false,
  "source": "https://github.com/Its4Nik/DockStacks",
  "compose_spec": {
    "services": {
      "uptime-kuma": {
        "image": "louislam/uptime-kuma:1",
        "container_name": "uptime-kuma",
        "restart": "always",
        "ports": ["3001:3001"],
        "volumes": ["./uptime-kuma-data:/app/data"]
      }
    }
  }
}
