{
	"include": [
		"**/*",
		"**/.server/**/*",
		"**/.client/**/*",
		".react-router/types/**/*"
	],
	"extends": ["../../tsconfig.base.json"],
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable", "ES2022"],
		"target": "ES2022",
		"composite": true,
		"module": "ES2022",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"paths": {
			"~/*": ["./app/*"],
			"@RR/routes/*": [".react-router/types/app/routes/+types/*"]
		},
		"rootDirs": [".", "./.react-router/types"],
		"baseUrl": ".",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true
	}
}
