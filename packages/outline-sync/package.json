{
  "name": "@dockstat/outline-sync",
  "version": "1.2.3",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "outline-sync": "dist/index.js"
  },
  "files": [
    "./dist/index.js",
    "./README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "dev": "bun run src/index.ts",
    "sync": "bun run src/index.ts sync",
    "watch": "bun run src/index.ts watch",
    "check-types": "bunx tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chokidar": "^4.0.0",
    "front-matter": "^4.0.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "bun-types": "latest"
  }
}
